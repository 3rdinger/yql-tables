<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
   <meta>
      <author>Lawrence Morrisroe (lem@yahoo-inc.com)</author>
      <description>Google Social Graph API</description>
      <documentationURL>http://code.google.com/apis/socialgraph/docs/lookup.html</documentationURL>
      <sampleQuery>SELECT * FROM {table} WHERE q='laaker.com' AND fme='1' AND edi='1';</sampleQuery>
   </meta>
   <bindings>
      <select itemPath="json" produces="JSON" >
         <urls>
            <url>http://socialgraph.apis.google.com/lookup</url>
         </urls>
         <inputs>
            <key id="q" type="xs:string" paramType="query" required="true" />
            <key id="edo" type="xs:boolean" paramType="query" required="false" />
            <key id="edi" type="xs:boolean" paramType="query" required="false" />
            <key id="fme" type="xs:boolean" paramType="query" required="false" />
            <key id="sgn" type="xs:boolean" paramType="query" required="false" />
            <key id="jme" type="xs:boolean" paramType="query" required="false" />
         </inputs>
      </select>
   </bindings>
</table>
