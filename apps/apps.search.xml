<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Micah Laaker (micah@laaker.com)</author>
    <description>Appolicious App Store Catalog Search</description>
    <documentationURL>http://appolicious.com/</documentationURL>
    <sampleQuery></sampleQuery>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://api.appolicious.com/api/{partner}/{version}/{facet}/{lang}/{country}/{platform}</url>
      </urls>
      <inputs>
		<key type="xs:string" paramType="path" default="ymobile" id="partner" />
		<key type="xs:string" paramType="path" default="v1" id="version" />
		<key type="xs:string" paramType="path" default="search" id="facet" />
		<key type="xs:string" paramType="path" default="en" id="lang" />
		<key type="xs:string" paramType="path" default="us" id="country" />
		<key type="xs:string" paramType="path" default="iphone" id="platform" />
        <key id="q" type="xs:string" paramType="query"/>
      </inputs>
    </select>
  </bindings>
</table>