<?xml version="1.0" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>Ryuichi Okumura</author>
        <documentationURL>http://developer.yahoo.co.jp/webapi/jlp/ma/v1/parse.html</documentationURL>
    </meta>
    <bindings>
        <select itemPath="ResultSet.ma_result" produces="XML">
            <urls>
                <url>http://jlp.yahooapis.jp/MAService/V1/parse</url>
            </urls>
            <inputs>
                <key id="appid" type="xs:string" paramType="query" required="true" />
                <key id="sentence" type="xs:string" paramType="query" required="true" />
                <key id="results" type="xs:string" paramType="query" required="true" />
                <key id="response" type="xs:string" paramType="query" required="false" />
                <key id="filter" type="xs:string" paramType="query" required="false" />
                <key id="ma_response" type="xs:string" paramType="query" required="false" />
                <key id="ma_filter" type="xs:string" paramType="query" required="false" />
                <key id="uniq_response" type="xs:string" paramType="query" required="false" />
                <key id="uniq_filter" type="xs:string" paramType="query" required="false" />
                <key id="uniq_by_baseform" type="xs:string" paramType="query" required="false" />
            </inputs>
        </select>
    </bindings>
</table>
