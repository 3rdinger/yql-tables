<?xml version="1.0" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>Ryuichi Okumura</author>
        <documentationURL>http://developer.yahoo.co.jp/webapi/chiebukuro/chiebukuro/v1/categorytree.html</documentationURL>
    </meta>
    <bindings>
        <select itemPath="ResultSet.Result" produces="XML">
            <urls>
                <url>http://chiebukuro.yahooapis.jp/Chiebukuro/V1/categoryTree</url>
            </urls>
            <inputs>
                <key id="appid" type="xs:string" paramType="query" required="true" />
                <key id="categoryid" type="xs:string" paramType="query" required="false" />
            </inputs>
        </select>
    </bindings>
</table>
