<?xml version="1.0" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>Ryuichi Okumura</author>
        <documentationURL>http://developer.yahoo.co.jp/webapi/shopping/shopping/v1/eventsearch.html</documentationURL>
    </meta>
    <bindings>
        <select itemPath="ResultSet.Result" produces="XML">
            <urls>
                <url>http://shopping.yahooapis.jp/ShoppingWebService/V1/eventSearch?appid={appid}&amp;affiliate_type={affiliate_type}&amp;affiliate_id={affiliate_id}&amp;event_type={event_type}&amp;event_id={event_id}&amp;sort={sort}&amp;hits={hits}&amp;offset={offset}</url>
            </urls>
            <inputs>
                <key id="appid" type="xs:string" paramType="path" required="true" />
                <key id="affiliate_type" type="xs:string" paramType="path" required="false" />
                <key id="affiliate_id" type="xs:string" paramType="path" required="false" />
                <key id="event_type" type="xs:string" paramType="path" required="false" />
                <key id="event_id" type="xs:string" paramType="path" required="false" />
                <key id="sort" type="xs:string" paramType="path" required="false" />
                <key id="hits" type="xs:string" paramType="path" required="false" />
                <key id="offset" type="xs:string" paramType="path" required="false" />
            </inputs>
        </select>
    </bindings>
</table>
