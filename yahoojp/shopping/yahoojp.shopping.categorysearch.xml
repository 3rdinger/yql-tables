<?xml version="1.0" ?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>Ryuichi Okumura</author>
        <documentationURL>http://developer.yahoo.co.jp/webapi/shopping/shopping/v1/categorysearch.html</documentationURL>
    </meta>
    <bindings>
        <select itemPath="ResultSet.Result" produces="XML">
            <urls>
                <url>http://shopping.yahooapis.jp/ShoppingWebService/V1/categorySearch?appid={appid}&amp;affiliate_type={affiliate_type}&amp;affiliate_id={affiliate_id}&amp;category_id={category_id}</url>
            </urls>
            <inputs>
                <key id="appid" type="xs:string" paramType="path" required="true" />
                <key id="affiliate_type" type="xs:string" paramType="path" required="false" />
                <key id="affiliate_id" type="xs:string" paramType="path" required="false" />
                <key id="category_id" type="xs:string" paramType="path" required="true" />
            </inputs>
        </select>
    </bindings>
</table>
