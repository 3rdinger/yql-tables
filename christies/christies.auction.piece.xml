<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd" https="false">
<meta>
    <author>Daniel Pett</author>
    <description>Search Christies auction</description>
    <documentationURL>http://www.finds.org.uk</documentationURL>
    <sampleQuery>select * from {table} where intobjectID = 5358441;</sampleQuery>
</meta>
<bindings>
   <select itemPath="json.items" produces="JSON">
        <urls>
            <url>http://www.christies.com/LotFinder/lot_details.aspx</url>
        </urls>
	<inputs>
      	    <key id="intObjectID" type="xs:string" paramType="query" required="true" />
	    <key id="xpath" type="xs:string" default="/html/body/div/div[2]/div/div/div/div/div[3]/ul[2]/li/p" /> 
        </inputs>
   </select>
</bindings>
</table>