<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Mike Dillon</author>
    <sampleQuery>select * from {table} where username = 'Jimbo Wales'</sampleQuery>
    <!--<documentationURL></documentationURL>-->
  </meta>
  <bindings>
    <select itemPath="api.query.usercontribs.item" produces="XML">
      <urls>
        <url>http://en.wikipedia.org/w/api.php?action=query&amp;format=xml&amp;list=usercontribs&amp;ucuser={username}</url>
      </urls>
<!--
      <paging model="offset">
        <start default="0" id="dummy"/>
        <pagesize max="10000" id="count"/>
        <total default="10"/>
      </paging>
-->
      <inputs>
        <key id="username" type="xs:string" paramType="path" required="true"/>
      </inputs>
    </select>
  </bindings>
</table>
