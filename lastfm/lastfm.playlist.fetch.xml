<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Playlist.fetch API method. Fetch XSPF playlists using a lastfm playlist url.</description>
    <documentationURL>http://www.last.fm/api/show?service=271</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=playlist.fetch</url>
      </urls>
      <inputs>
        <key id="playlistURL" type="xs:string" paramType="query" required="true" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
