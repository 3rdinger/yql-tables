<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm User.getPastEvents API method. Get a paginated list of all events a user has attended in the past.</description>
    <documentationURL>http://www.last.fm/api/show?service=343</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=user.getpastevents</url>
      </urls>
      <inputs>
        <key id="user" type="xs:string" paramType="query" required="true" />
        <key id="page" type="xs:string" paramType="query" required="false" />
        <key id="limit" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
