<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Geo.getTopTracks API method. Get the most popular tracks on Last.fm last week by country</description>
    <documentationURL>http://www.last.fm/api/show?service=298</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=geo.gettoptracks</url>
      </urls>
      <inputs>
        <key id="country" type="xs:string" paramType="query" required="true" />
        <key id="location" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
