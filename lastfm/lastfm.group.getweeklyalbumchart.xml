<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Group.getWeeklyAlbumChart API method. Get an album chart for a group, for a given date range. If no date range is supplied, it will return the most recent album chart for this group.</description>
    <documentationURL>http://www.last.fm/api/show?service=293</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=group.getweeklyalbumchart</url>
      </urls>
      <inputs>
        <key id="user" type="xs:string" paramType="query" required="true" />
        <key id="from" type="xs:string" paramType="query" required="false" />
        <key id="to" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
