<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Tag.getSimilar API method. Search for tags similar to this one. Returns tags ranked by similarity, based on listening data.</description>
    <documentationURL>http://www.last.fm/api/show?service=311</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=tag.getsimilar</url>
      </urls>
      <inputs>
        <key id="tag" type="xs:string" paramType="query" required="true" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
