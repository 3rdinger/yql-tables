<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Geo.getMetroUniqueArtistChart API method. Get a chart of the artists which make that metro unique</description>
    <documentationURL>http://www.last.fm/api/show?service=424</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=geo.getmetrouniqueartistchart</url>
      </urls>
      <inputs>
        <key id="country" type="xs:string" paramType="query" required="true" />
        <key id="metro" type="xs:string" paramType="query" required="true" />
        <key id="start" type="xs:string" paramType="query" required="false" />
        <key id="end" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
