<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Track.getInfo API method. Get the metadata for a track on Last.fm using the artist/track name or a musicbrainz id.</description>
    <documentationURL>http://www.last.fm/api/show?service=356</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=track.getinfo</url>
      </urls>
      <inputs>
        <key id="artist" type="xs:string" paramType="query" required="false" />
        <key id="track" type="xs:string" paramType="query" required="false" />
        <key id="mbid" type="xs:string" paramType="query" required="false" />
        <key id="username" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
