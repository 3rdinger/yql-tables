<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Tag.getTopAlbums API method. Get the top albums tagged by this tag, ordered by tag count.</description>
    <documentationURL>http://www.last.fm/api/show?service=283</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=tag.gettopalbums</url>
      </urls>
      <inputs>
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
