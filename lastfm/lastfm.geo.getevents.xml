<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Geo.getEvents API method. Get all events in a specific location by country or city name.</description>
    <documentationURL>http://www.last.fm/api/show?service=270</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=geo.getevents</url>
      </urls>
      <inputs>
        <key id="location" type="xs:string" paramType="query" required="false" />
        <key id="lat" type="xs:string" paramType="query" required="false" />
        <key id="long" type="xs:string" paramType="query" required="false" />
        <key id="page" type="xs:string" paramType="query" required="false" />
        <key id="distance" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
        
      </inputs>
    </select>
  </bindings>
</table>
