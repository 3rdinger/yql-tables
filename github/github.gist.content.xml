<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Martin Wittemann</author>
    <author>Fabian Jakobs</author>
    <documentationURL></documentationURL>
    <sampleQuery>select * from {table} where gist='276994'</sampleQuery>
  </meta>
  <bindings>
    <select itemPath="gist" produces="XML">
      <urls>
        <url>http://gist.github.com/{gist}.txt</url>
      </urls>
      <inputs>
        <key id="gist" type="xs:string" paramType="path" required="true"/>
      </inputs>
        <execute><![CDATA[
	  var gist = request.get().response;
          response.object = <gist>{gist}</gist>;
	]]></execute>
    </select>
  </bindings>
</table>