<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Ethan Jewett</author>
    <documentationURL></documentationURL>
	<sampleQuery>select * from esme.restapi.messages WHERE esme_api_endpoint="http://esmecloudserverapache.dickhirsch.staxapps.net/api" AND cookie="JSESSIONID=EAD4C83F772A3FF6252BA5C4C4910CF8; Path=/"</sampleQuery>
  </meta>
  <bindings>
	<select itemPath="" produces="XML">  
	  <urls>  
	    <url></url>  
	  </urls>  
	  <inputs>  
	    <key id="esme_api_endpoint" type="xs:string" required="true" paramType="variable"/>  
	    <key id="cookie" type="xs:string" required="true" paramType="variable"/>  
      </inputs>  
	  <execute><![CDATA[ 
	       response.object = y.rest(esme_api_endpoint)
								.path("get_msgs")
								.header("Cookie",cookie)	
								.get()
								.response;
	      ]]>
	  </execute>  
	</select>
  </bindings>
</table>