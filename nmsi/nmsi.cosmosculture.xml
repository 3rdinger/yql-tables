<?xml version="1.0"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jim O'Donnell</author>
    <documentationURL>http://cosmiccollections.pbworks.com/Cosmic-Collections-API</documentationURL>
  </meta>
  <bindings>
    <select produces="XML" itemPath="MuseumObject">
      <urls>

        <url>http://www.sciencemuseum.org.uk/objectapi/cosmosculturepublic.svc/MuseumObjects/AccessionNumber/{AccessionNumber}</url>
      </urls>
      <inputs>
        <key id="AccessionNumber" type="xs:string" paramType="path" required="true"/>
      </inputs>
    </select>
	<select produces="XML" itemPath="MuseumObjectsResponse.MuseumObjectsResult.MuseumObject">
      <urls>

        <url>http://www.sciencemuseum.org.uk/objectapi/cosmosculturepublic.svc/MuseumObjects</url>
      </urls>
      <inputs>
      </inputs>
    </select>
  </bindings>
</table>